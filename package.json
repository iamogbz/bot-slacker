{
  "name": "slacker-bot",
  "version": "0.1.1",
  "description": "The awesomest bot evar.",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/lib/**"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamogbz/bot-slacker.git"
  },
  "keywords": [
    "slack",
    "bot"
  ],
  "author": "Emmanuel Ogbizi-Ugbe",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/iamogbz/bot-slacker/issues"
  },
  "homepage": "https://github.com/iamogbz/bot-slacker#readme",
  "engines": {
    "node": ">=10.4.0",
    "npm": ">=6.1.0"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "botkit": "^0.7.4",
    "dotenv": "^7.0.0",
    "moment": "^2.22.2",
    "mongodb": "^3.2.2",
    "rxjs": "^6.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2"
  }
}
